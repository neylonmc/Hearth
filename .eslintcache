el\"\n      >\n        <ButtonGroup className=\"button-container\">\n            { Entertainment.map(data => {\n              if (data.topic === topic) {\n                return(\n                  <Col \n                    id=\"results-container\"\n                    className=\"animate__animated animate__fadeIn\"\n                    md={4} \n                    sm={12}\n                  >\n                      <Button  \n                        id=\"results-button\"\n                        href={ data.local_ext }\n                        bordered\n                      >\n                          <img \n                              id= \"topic-image\"\n                              src= { data.img }\n                              alt= { data.title }\n                          />\n                      </Button>\n                  </Col>\n\n                );\n              } \n            }) }\n        </ButtonGroup>\n      </Panel>\n    </div>\n  )\n\n}\n\n\n  \n\n","/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Profile-Topic/Topic.js",["145"],"import React from 'react';\nimport {\n    useParams\n  } from \"react-router-dom\";\nimport { \n    Row,\n    Col,\n    PanelGroup,\n    Panel\n} from \"rsuite\";\nimport \"./Profile-Topic.css\";\nimport Entertainment from \"../../utils/TopEntertainment\";\nimport API from \"../../utils/API\";\nimport TopicInfo from \"../../components/TopicInfo/TopicInfo\";\nimport Activity from \"../../components/Activity/Activity\";\n\nfunction Topic() {\n    \n    let { topic } = useParams();\n\n    API.getActivities()\n        .then((res) => {\n            console.log(res)\n        }).catch(err => err)\n\n\n    return(\n        Entertainment.map((data) => {\n            if (data.ext === topic ) {\n                console.log(data.title);\n\n                return(\n                    <div className=\"page-container animate__animated animate__fadeIn\">\n                        <Row className=\"header-container\">\n                            <Col sm= {3} >\n                                <img \n                                    className = \"page-image\"\n                                    src= { data.img } \n                                    alt= \"icon of topic poster\"\n                                />\n                            </Col>\n                            <Col sm= {21}>\n                                <Row>\n                                    <h3 id=\"sub-header\" className=\"page-title\">{data.title}</h3>\n                                </Row>\n                                <Row>\n                                    <h4 className=\"topic-head\">{data.topic} | { data.genre.join(\", \") }</h4>\n                                </Row>\n                            </Col>\n                        </Row>\n\n                        <PanelGroup \n                            className= \"panel-container\"\n                            accordion \n                        >\n                            <Panel \n                                id=\"panel\"\n                                className=\"about-panel\"\n                                header=\"about\"\n                                defaultExpanded\n                            >\n                                <TopicInfo \n                                    topic={data.topic}\n                                    ageRange= {data.age_range.join(\", \")}\n                                    ageRating= {data.age_rating}\n                                    genre={data.genre.join(\", \")}\n                                    description={data.description}\n                                />\n                            </Panel>\n                            <Panel \n                                id=\"panel\"\n                                className=\"activity-panel\"\n                                header=\"activity\" \n                                defaultExpanded\n                            >\n                                <Activity />\n                            </Panel>\n                        </PanelGroup>\n\n                    </div>\n                )\n            } \n        })\n    )\n};\n\nexport default Topic;","/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/components/Nav/Nav.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Dashboard/Dashboard.js",["146","147","148"],"import React, { Component } from \"react\";\nimport { \n  Row,\n  Col,\n  Panel, \n  Button, \n  ButtonToolbar, \n  ButtonGroup } from \"rsuite\";\nimport \"./Dashboard.css\";\nimport Streaming from \"../../components/Streaming/Streaming\";\nimport SimilarUsers from \"../../components/SimilarUsers/SimilarUsers\";\n// import movie from \"../assets/images/movieIcon.jpeg\";\n// import book from \"../assets/images/bookIcon.jpg\";\n// import tv from \"../assets/images/tvIcon.jpg\";\n// import musicIcon from \"../assets/images/musicIcon.jpeg\";\n\nexport class Dashboard extends Component {\n  render() {\n    return (\n      <div className=\"dashboard-container  animate__animated animate__fadeIn\">\n        {/* INITIAL BLOCKS */}\n        <Row>\n          {/* USER INFO */}\n          <Col xs={7} id=\"block-container\">\n            <Panel\n              id=\"left-block\"\n              className=\"user-container\"\n              bordered\n            >\n              <Row>\n                <Col>\n                  <img\n                    className=\"dash-avatar\"\n                    src=\"./images/no-avatar.jpg\"\n                    alt=\"dashboard user avatar\"\n                  />\n                </Col>\n              </Row>\n\n              <Row>\n                <Col>\n                   <h1 className=\"dash-username\">username</h1>\n                </Col>\n              </Row>\n\n              <Streaming />\n\n            </Panel>\n\n            {/* POLLS */}\n            <Panel\n              id=\