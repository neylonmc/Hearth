\n\n  getUser() {\n    axios.get('/user/').then(response => {\n      console.log('Get user response: ')\n      console.log(response.data)\n      if (response.data.user) {\n        console.log('Get User: There is a user saved in the server session: ')\n\n        this.setState({\n          loggedIn: true,\n          username: response.data.user.username\n        })\n      } else {\n        console.log('Get user: no user');\n        this.setState({\n          loggedIn: false,\n          username: null\n        })\n      }\n    })\n  };\n\n  render() {\n    return (\n      <Router>\n        <div className=\"app\">\n          <Nav />\n          <Container>\n            <Switch>\n              <Route exact path={\"/\"} component={Home} />\n              <Route exact path=\"/login\" render={() =><LoginForm updateUser={this.updateUser} />} /> \n              <Route exact path=\"/signup\" render={() => <SignUpForm />} />\n              <Route exact path=\"/select\" component={Select}/>\n              <Route path=\"/topics\" component={Browse} /> \n              <Route exact path=\"/dashboard\" component={Dashboard} />\n              <Route exact path=\"/profile\" component={Profile} />\n              <Route exact path=\"/topic\" component={Topic} />\n              <Route exact path=\"/settings\" component={Settings} />\n              <Route path=\"*\"  component={NoMatch} />\n            </Switch>\n          </Container>\n        </div>\n      </Router>\n    );\n  }\n};\n\nexport default App;\n","/Users/dylanfrank/Desktop/Hearth/Hearth/src/pages/Login/Login.js",[],"/Users/dylanfrank/Desktop/Hearth/Hearth/src/pages/Dashboard/Dashboard.js",[],"/Users/dylanfrank/Desktop/Hearth/Hearth/src/pages/Profile-Topic/Profile.js",[],"/Users/dylanfrank/Desktop/Hearth/Hearth/src/pages/Profile-Topic/Topic.js",["161"],"import React from 'react';\nimport {\n    useParams\n  } from \"react-router-dom\";\nimport { \n    Row,\n    Col,\n    PanelGroup,\n    Panel\n} from \"rsuite\";\nimport \"./Profile-Topic.css\";\nimport Entertainment from \"../../utils/TopEntertainment\";\nimport API from \"../../utils/API\";\nimport TopicInfo from \"../../components/TopicInfo/TopicInfo\";\nimport Activity from \"../../components/Activity/Activity\";\n\nfunction Topic() {\n    \n    let { topic } = useParams();\n\n    API.getActivities()\n        .then((res) => {\n            console.log(res)\n        }).catch(err => err)\n\n\n    return(\n        Entertainment.map((data) => {\n            if (data.ext === topic ) {\n                console.log(data.title);\n\n                return(\n                    <div className=\"page-container animate__animated animate__fadeIn\">\n                        <Row className=\"header-container\">\n                            <Col sm= {3} >\n                                <img \n                                    className = \"page-image\"\n                                    src= { data.img } \n                                    alt= \"icon of topic poster\"\n                                />\n                            </Col>\n                            <Col sm= {21}>\n                                <Row>\n                                    <h3 id=\"sub-header\" className=\"page-title\">{data.title}</h3>\n                                </Row>\n                                <Row>\n                                    <h4 className=\"topic-head\">{data.topic} | { data.genre.join(\", \") }</h4>\n                                </Row>\n                            </Col>\n                        </Row>\n\n                        <PanelGroup \n                            className= \"panel-container\"\n                            accordion \n                        >\n                            <Panel \n                                id=\"panel\"\n                                className=\"about-panel\"\n                                header=\"about\"\n                                defaultExpanded\n                            >\n                                <TopicInfo \n                                    topic={data.topic}\n                                    ageRange= {data.age_range.join(\", \")}\n                                    ageRating= {data.age_rating}\n                                    genre={data.genre.join(\", \")}\n                                    description={data.description}\n                                />\n                            </Panel>\n                            <Panel \n                                id=\"panel\"\n                                className=\"activity-panel\"\n                                header=\"activity\" \n                                defaultExpanded\n                            >\n                                <Activity />\n                            </Panel>\n                        </PanelGroup>\n\n                    </div>\n                )\n            } \n        })\n    )\n};\n\nexport default Topic;","/Users/dylanfrank/Desktop/Hearth/Hearth/src/pages/NoMatch/NoMatch.js",[],"/Users/dylanfrank/Desktop/Hearth/Hearth/src/pages/Settings/Settings.js",[],"/Users/dylanfrank/Desktop/Hearth/Hearth/src/pages/Browse-Topics/Browse.js",["162"],"import React from \"react\";\nimport {\n  Switch,\n  Route,\n  Link,\n  useParams,\n  useRouteMatch\n} from \"react-router-dom\";\nimport { \n  ButtonGroup,\n  Row,\n  Col,\n  Panel,\n  Button\n} from \"rsuite\";\nimport \"rsuite/lib/styles/index.less\";\nimport \"./Browse.css\";\nimport allTopics from \"../../utils/Topics\";\nimport Entertainment from \"../../utils/TopEntertainment\";\n\nexport default function Topics() {\n  let { path, url } = useRouteMatch();\n\n  return (\n    <div className=\"browse-container  animate__animated animate__fadeIn\">\n      <h3 className=\"browse-title\">browse by topic</h3>\n      <Row className=\"topic-links\">\n        { allTopics.map(topic => {\n            return( \n              <Col id=\"link-container\" md={6} sm={12}>\n                <Link \n                  id=\"topic-button\"\n                  to={`${url}/${topic.topic}`}\n                >\n                  {topic.topic}\n                </Link>\n              </Col>\n\n            )\n          })\n        }\n      </Row>\n      \n      <Switch>\n        <Route exact path={path}>\n          <div className=\"empty-container\"/>\n        </Route>\n        <Route exact path={`${path}/:topic`}>\n          <Topic />\n        </Route>\n      </Switch>\n    </div>\n  );\n};\n\nfunction Topic() {\n\n  let { topic } = useParams();\n\n  return (\n    <div className=\"topic-container  animate__animated animate__fadeIn\">\n      <Panel\n        className=\"topic-panel\"\n      >\n        <ButtonGroup className=\"button-container\">\n            { Entertainment.map(data => {\n              if (data.topic === topic) {\n                return(\n                  <Col \n                    id=\"results-container\"\n                    className=\"animate__animated animate__fadeIn\"\n                    md={4} \n                    sm={12}\n                  >\n                      <Button  \n                        id=\"results-button\"\n                        href={ data.local_ext }\n                        bordered\n                      >\n                          <img \n                              id= \"topic-image\"\n                              src= { data.img }\n                              alt= { data.title }\n                          />\n                      </Button>\n                  </Col>\n\n                );\n              } \n            }) }\n        </ButtonGroup>\n      </Panel>\n    </div>\n  )\n\n}\n\n\n  \n\n","/Users/dylanfrank/Desktop/Hearth/Hearth/src/pages/Select/Select.js",[],"/Users/dylanfrank/Desktop/Hearth/Hearth/src/components/CreateNew/CreateNew.js",["163","164","165","166","167"],"/Users/dylanfrank/Desktop/Hearth/Hearth/src/components/Nav/Nav.js",[],"/Users/dylanfrank/Desktop/Hearth/Hearth/src/components/LoginFor