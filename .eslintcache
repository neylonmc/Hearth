 */}\n            <Panel\n              id=\"left-block\"\n              className=\"similar-container\"\n              bordered\n            >\n              <h2>similar users</h2>\n              <SimilarUsers />\n            </Panel>\n\n          </Col>\n\n        {/* DASHBOARD ACTIVITY */}\n          <Col xs={17} id=\"block-container\">\n            <Panel\n              className=\"activity-container\"\n            >\n              <h2 className=\"activity-title\">start browsing popular entertainment...</h2>\n              <DashActivity />\n            </Panel>\n          </Col>\n\n        </Row>\n\n      </div>\n    );\n};\n\nexport default Dashboard;\n","/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Browse-Topics/Browse.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/NewPost/NewPost.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Profile-Topic/Profile.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Profile-Topic/Topic.js",["176","177"],"import React, { useState, useEffect } from \"react\";\nimport {\n    useParams\n} from \"react-router-dom\";\nimport {\n    Row,\n    Col,\n    PanelGroup,\n    Panel\n} from \"rsuite\";\nimport \"./Profile-Topic.css\";\nimport API from \"../../utils/API\";\nimport TopicInfo from \"../../components/TopicInfo/TopicInfo\";\nimport TopicActivity from \"../../components/Activity/TopicActivity\";\nimport FollowButton from \"../../components/FollowButton/FollowButton\";\n\nfunction Topic() {\n\n    let { topic } = useParams();\n\n    const [commentsObject, setData] = useState([]);\n    const [topicObject, setTopic] = useState({\n        category: []\n\n    });\n\n    useEffect(() => {\n        console.log(topic)\n        loadActivities(topic);\n    },[]);\n\n    function loadActivities(topic) {\n        API.getActivitybyTitle(topic)\n            .then((res) => {\n                setTopic(res.data[0])\n                console.log(topicObject);\n                API.getCommentsByActivity(res.data[0]._id)\n                    .then((res) => {\n                        setData(res.data);\n                    }).catch(err => err)\n            }).catch(err => err)\n    }\n\n    console.log(topicObject);\n    return (\n        <div className=\"page-container animate__animated animate__fadeIn\">\n            <Row className=\"header-container\">\n                <Col md={24} >\n                    <img\n                        className=\"page-image\"\n                        src={topicObject.Image || \"./images/no-image.png\"}\n                        alt=\"icon of topic poster\"\n                    />\n                </Col>\n            </Row>\n            <Row className=\"follow-container\">\n                <Col md={24}>\n                    <FollowButton user={topicObject}/>\n                </Col>\n            </Row>\n            <Row>\n                <Col md={24}>\n                    <Row>\n                        <h3 id=\"sub-header\" className=\"page-title\">{topicObject.title}</h3>\n                    </Row>\n                    <Row>\n                        <h4 className=\"topic-head\">{topicObject.type} | { topicObject.category.join(\", \") }</h4>\n                    </Row>\n                </Col>\n            </Row>\n\n            <PanelGroup\n                className=\"panel-container\"\n                accordion\n            >\n                <Panel\n                    id=\"panel\"\n                    className=\"about-panel\"\n                    defaultExpanded\n                >\n                    <TopicInfo\n                        topic={topicObject.type}\n                        ageRange={topicObject.ageRange}\n                        genre={topicObject.category.join(\", \")}\n                        description={topicObject.description}\n                    />\n                </Panel>\n                <Panel\n                    id=\"panel\"\n                    className=\"activity-panel\"\n                    header=\"activity\"\n                    defaultExpanded\n                >\n                    <TopicActivity />\n                </Panel>\n            </PanelGroup>\n        </div>\n    )\n};\n\nexport default Topic;","/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Settings/Settings.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/NoMatch/NoMatch.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Login/Login.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Logout/Logout.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/About/About.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/components/Nav/Nav.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/components/SignUpForm/SignUpForm.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/components/Nav/MenuItem.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/utils/API.js",["178"],"import axios from \"axios\";\n\nexport default {\n  // Gets all activities\n  getActivities: function () {\n    return axios.get(\"/api/activities\");\n  },\n  // Gets the activity with the given id\n  getActivitybyTitle: function (title) {\n    return axios.get(\"/api/activities/\" + title);\n  },\n  // Deletes the activity with the given id\n  deleteActivity: function (id) {\n    return axios.delete(\"/api/activities/\" + id);\n  },\n  getActivitiesByType: function (type) {\n    return axios.get(\"/api/activities/type/\" + type);\n  },\n  getActivitiesByTypeHighestRated: function (type) {\n    return axios.get(\"/api/activities/highest/\" + type);\n  },\n  getActivitiesByTag: function (tag) {\n    return axios.get(\"/api/activities/tag/\" + tag);\n  },\n  // Saves an activity to the database