d={24}>\n                    <Row>\n                        <h3 id=\"sub-header\" className=\"page-title\">{topicObject.title}</h3>\n                    </Row>\n                    <Row>\n                        <h4 className=\"topic-head\">{topicObject.type} | { topicObject.category.join(\", \") }</h4>\n                    </Row>\n                </Col>\n            </Row>\n\n            <PanelGroup\n                className=\"panel-container\"\n                accordion\n            >\n                <Panel\n                    id=\"panel\"\n                    className=\"about-panel\"\n                    defaultExpanded\n                >\n                    <TopicInfo\n                        topic={topicObject.type}\n                        ageRange={topicObject.ageRange}\n                        genre={topicObject.category.join(\", \")}\n                        description={topicObject.description}\n                    />\n                </Panel>\n                <Panel\n                    id=\"panel\"\n                    className=\"activity-panel\"\n                    header=\"activity\"\n                    defaultExpanded\n                >\n                    <TopicActivity />\n                </Panel>\n            </PanelGroup>\n        </div>\n    )\n};\n\nexport default Topic;","/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Settings/Settings.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/NoMatch/NoMatch.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Login/Login.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/About/About.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/pages/Logout/Logout.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/components/Nav/Nav.js",[],"/Users/eaclumpkens/Documents/Dev/Bootcamp/inclass/Projects/Hearth/src/components/SignUp