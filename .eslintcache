                }).catch(err => err)\n            }).catch(err => err)\n    }\n\n\n\n    return (\n        commentsObject.map((data) => {\n            console.log(data.title);\n\n            return (\n                <div className=\"page-container animate__animated animate__fadeIn\">\n                    <Row className=\"header-container\">\n                        <Col sm={3} >\n                            <img\n                                className=\"page-image\"\n                                src={data.img}\n                                alt=\"icon of topic poster\"\n                            />\n                        </Col>\n                        <Col sm={21}>\n                            <Row>\n                                <h3 id=\"sub-header\" className=\"page-title\">{data.title}</h3>\n                            </Row>\n                            <Row>\n                                <h4 className=\"topic-head\">{topicObject.topic} | {topicObject.category.join(\", \")}</h4>\n                            </Row>\n                        </Col>\n                    </Row>\n\n                    <PanelGroup\n                        className=\"panel-container\"\n                        accordion\n                    >\n                        <Panel\n                            id=\"panel\"\n                            className=\"about-panel\"\n                            header=\"about\"\n                            defaultExpanded\n                        >\n                            <TopicInfo\n                                topic={data.topic}\n                                ageRange={topicObject.ageRange.join(\", \")}\n                                ageRating={\"\"}\n                                genre={topicObject.category.join(\", \")}\n                                description={data.description}\n                            />\n                        </Panel>\n                        <Panel\n                            id=\"panel\"\n                            className=\"activity-panel\"\n                            header=\"activity\"\n                            defaultExpanded\n                        >\n                            <Activity />\n                        </Panel>\n                    </PanelGroup>\n\n                </div>\n            )\n\n        })\n    )\n};\n\nexport default Topic;","/Users/dylanfrank/Desktop/hearth2/Hearth/src/pages/Profile-Topic/Profile.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/pages/Settings/Settings.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/pages/NoMatch/NoMatch.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/pages/Login/Login.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/pages/About/About.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/pages/Logout/Logout.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/components/SignUpForm/SignUpForm.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/components/Nav/Nav.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/components/Nav/MenuItem.js",[],"/Users/dylanfrank/Desktop/hearth2/Hearth/src/components/SimilarUsers/SimilarUsers.js",["171"],"import React, { useState, useEffect } from 'react';\nimport axios from 'axios';\nimport {\n    // Row,\n    // Col,\n    Avatar\n} from 'rsuite';\nimport \"./SimilarUsers.css\";\nimport API from \"../../utils/RandomAPI\";\n\nfunction SimilarUsers() {\n    // DUMBY DATA\n    const [searchState, setSearch] = useState([]);\n    useEffect(() => {\n        API.randomUsers().then(users => {\n            console.log(users.data.results);\n            setSearch(users.data.results);\n        });\n    }, []);\n\n    return(\n\n        searchState.map(user => {\n            return( \n                <Avatar id=\"avatar\" size=\"lg\" src={user.picture.medium}>\n                    {user.name.first[0]}{user.name.last[0]}\n                </Avatar>\n            )\n\n        })\n    )\n\n    \n\n};\n\nexport default SimilarUsers;","/Use