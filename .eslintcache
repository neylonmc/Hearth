 \n                rows={5} \n                name=\"textarea\" \n                placeholder=\"review title\"\n                onChange={\n                    function(value) {\n                        setTitle(value)\n                    }\n                }\n            />\n          </FormGroup>\n\n          {/* COMMENT REVIEW */}\n          <FormGroup>\n            <FormControl \n                rows={5} \n                className=\"review-text\" \n                name=\"textarea\" \n                placeholder=\"my review\"\n                onChange={\n                    function(value) {\n                        setText(value)\n                    }\n                }\n            />\n          </FormGroup>\n\n          {/* REVIEW RATING */}\n          <FormGroup>\n            <Rate \n                className=\"review-rating\" \n                onChange={\n                    function(value) {\n                        setRate(value);\n                    }\n                }\n                defaultValue={3.5} \n                allowHalf \n            />\n          </FormGroup>\n\n          {/* SUBMIT BUTTON */}\n          <ButtonToolbar>\n            <Button \n                className=\"submit-button\"  \n                appearance=\"primary\"\n                onClick={\n                    function() {\n                        setReview({\n                            topic: topicState, \n                            title: titleState,\n                            review: textState,\n                            rating: rateState\n                        })\n\n                        console.log(reviewState);\n                    }\n                }\n            >\n              submit\n            </Button>\n          </ButtonToolbar>\n        </Form>\n      </div>\n    </div>\n  );\n};\n\nexport default NewReview;\n","/Users/mneylon/Desktop/CF/project3/Hearth/src/utils/API.js",["157"],"import axios from \"axios\";\n\nexport default {\n  // Gets all acti